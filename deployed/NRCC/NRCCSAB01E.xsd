<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/NRCCSAB01E" version="2022.0.000" xmlns="http://www.lmonte.com/besm/NRCCSAB01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCCSABE"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CERTIFICATE OF COMPLIANCE NR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Solar And Battery"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCC"/>
                     <xsd:element ref="nRCCSAB01E"/>
                     <xsd:element name="DeclarationsBlock" type="comp:DeclarationsNRCC" maxOccurs="unbounded"/>
                     <xsd:element name="Footer" type="comp:FooterNRCC"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCCSABE"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCCSAB01E">
      <xsd:annotation>
         <xsd:documentation>Solar And Battery</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>A. GENERAL INFORMATION</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This document is used to demonstrate compliance with prescriptive PV and battery requirements in 
                              <d:t24refnolink/>140.10/
                              <d:t24refnolink/>170.2 for nonresidential, multifamily and mixed-use buildings and prescriptive solar thermal requirements in 
                              <d:t24refnolink/>170.2(d)3C for multifamily and hotel/ motel occupancies.
When PV/battery/solar thermal requirements don't apply or are traded using the performance approach, this document demonstrates compliance with mandatory solar readiness requirements in 
                              <d:t24refnolink/>110.10/ 
                              <d:t24refnolink/>160.8 for newly constructed buildings which are either multifamily ten stories or fewer, hotel/motel ten stories or fewer or all other nonresidential buildings three stories or fewer.  It is also used to demonstrate compliance with solar readiness in 
                              <d:t24refnolink/>110.10/ 
                              <d:t24refnolink/>160.8 for additions to nonresidential, multifamily or hotel/motel building types which add more than 2,000 ft<d:sup>2</d:sup> of roof area.  Alterations, or additions of less than 2,000 ft<d:sup>2</d:sup> of roof area, are not required to comply with solar readiness, solar PV and battery requirements and do not need to complete this document.
 </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ClimateZone" type="com:ClimateZone">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from list 1-16</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_ConditionedFloorArea" type="com:ConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>STOP! Roofs designed for vehicle traffic, parking or for heliports are not required to comply with solar ready requirements in 
                                 <d:t24ref h="110.10"/>.  Document this exception by providing the plan sheet showing the roof design in 03a to the right and completing the signature page.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_WholeBuildingOccupancy" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Occupancies</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="HighRiseResidential">High-rise multifamily</dtyp:displayterm>
                           <dtyp:displayterm value="SchoolBuilding">School or Classroom</dtyp:displayterm>
                           <dtyp:displayterm value="MedicalClinicBuilding">Medical Office Bldg/ Clinic</dtyp:displayterm>
                           <dtyp:displayterm value="Hospital">I-2 and I-2.1 Healthcare Facilities</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:WholeBuildingOccupancy">
                           <xsd:enumeration value="AllOthersBuildings"/>
                           <xsd:enumeration value="AuditoriumBuilding"/>
                           <xsd:enumeration value="CommercialIndustrialBuilding"/>
                           <xsd:enumeration value="ConventionCenterBuilding"/>
                           <xsd:enumeration value="DataCenterBuilding"/>
                           <xsd:enumeration value="FinancialInstitutionBuilding"/>
                           <xsd:enumeration value="GroceryStoreBuilding"/>
                           <xsd:enumeration value="GymnasiumBuilding"/>
                           <xsd:enumeration value="HighRiseResidential"/>
                           <xsd:enumeration value="HospitalBuilding"/>
                           <xsd:enumeration value="HotelMotel"/>
                           <xsd:enumeration value="LibraryBuilding"/>
                           <xsd:enumeration value="MedicalClinicBuilding"/>
                           <xsd:enumeration value="OfficeBuilding"/>
                           <xsd:enumeration value="ParkingGarageBuilding"/>
                           <xsd:enumeration value="ReligiousFacilityBuilding"/>
                           <xsd:enumeration value="RelocatablePublicSchoolBuilding"/>
                           <xsd:enumeration value="RestaurantBuilding"/>
                           <xsd:enumeration value="RetailBuilding"/>
                           <xsd:enumeration value="SchoolBuilding"/>
                           <xsd:enumeration value="SupportArea"/>
                           <xsd:enumeration value="TheaterBuilding"/>
                           <xsd:enumeration value="UnleasedTenantSpace"/>
                           <xsd:enumeration value="Warehouse"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A05_RoofConstRangeSolar" type="env:RoofConstRangeSolar">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Construction Type</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from list</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_BuildingStoryRange" type="com:BuildingStoryRange">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Number of Stories</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05Message" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>STOP!  Alterations or additions that increase roof area by 
                                 <d:lte/> 2,000 ft<d:sup>2</d:sup> are not required to comply with requirements in 
                                 <d:t24refnolink/>110.10/ 
                                 <d:t24refnolink/>140.10/ 
                                 <d:t24refnolink/>170.2(g and h) and do not need to complete this compliance document.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This message is displayed in User interface  if   A05_RoofConstRangeSolar == AddAltRoofLTE2000ft2</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04Message" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>STOP!  Healthcare facilities are not required to comply with the requirements in 
                                 <d:t24refnolink/>110.10/ 
                                 <d:t24refnolink/>140.10/ 
                                 <d:t24refnolink/>170.2(g and h) and do not need to complete this compliance document.</d:i>
                           </d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_RequestedMMB" type="comp:RequestedMMB" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is a flag set by the agent that generates the report XML data to indicate that the user requested a Mandatory Measures Note Block which is placed after the DeclarationBlock in the XML output.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="AErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b> B. PROJECT SCOPE</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>The compliance path the project is using to comply per 
                              <d:t24refnolink/>110.10(b)1B/ 
                              <d:t24refnolink/>140.10/ 
                              <d:t24refnolink/>170.2(g and h) is indicated below. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BSolarReady" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance with Solar Readiness Requirements in 
                           <d:t24refnolink/>110.10(b)1B</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If this subsection is required,  one and only one item can be checked with a result == true and it is the only item that is displayed in the final report.
 
Conditions when this subsection is required or not required
 
If A04_WholeBuildngOccupancy == MedicalClinicBuilding
BSolarReady is NOT REQUIRED
 
Else If Bpv Is not required and NotPresent
BSolarReady is REQUIRED
Else If Bpv Is required and IsPresent And 
Bpv01b_AccessAreaTooSmall == true OR 
Bpv01c_RequiredLT4kW == true OR 
Bpv01d_NoContiguousAccess OR
Bpv01e_SnowloadException == true Or 
Bpv0f_MultitenantException == true Or 
Bpv0g_PerfTradeOffException == true
BSolarReady is REQUIRED</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="B01a_IsProjectAllocatedSolarZone" type="comp:IsProjectAllocatedSolarZone">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Provide Solar Ready Area no exceptions</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project has allocated a solar zone on the roof plan per requirements in 
                                    <d:t24ref h="110.10(b)"/>, as documented in Table F.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">By default result = false and unchecked.  If user checks it, the result = true,
 
If result == false NOT REPORTED IN PDF
 
Mandatory Measure Triggers:
If result == true
MM110_10b1B_MeasureApplies  = true and is displayed And 
MM110_10b4_MeasureApplies  = true and is displayed And 
DUMM110_10b2_MeasureApplies  = true and is displayed And 
DUMM110_10b3_MeasureApplies  = true and is displayed And 
DUMM110_10b4_MeasureApplies  = true and is displayed And 
DUMM110_10c_MeasureApplies  = true and is displayed And 
DUMM110_10d_MeasureApplies  = true and is displayed And 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="B01b_IsProjectInstalledPhotovoltaicSystem" type="comp:IsProjectInstalledPhotovoltaicSystem">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to Solar Ready Area: Installed Solar Photovoltaic System</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project includes a permanently installed solar electric system having a nameplate DC power rating, measured under Standard Test Conditions, of no less than one watt per square foot of roof area as documented in Table G.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">By default result = false and unchecked.  If user checks it, the result = true,  
If result == true Section G is triggered 
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="B01c_IsProjectInstalledSolarWaterHeating" type="comp:IsProjectInstalledSolarWaterHeating">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Exception to Solar Ready Area: Installed Solar Water Heating System</xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">The project  is a hotel/motel or high-rise multifamily occupancy and includes a permanently installed domestic solar water-heating system complying with 
                                       <d:t24refnolink/>170.2(d)3C and Reference Residential Appendix RA4, as documented in Table H.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If A04_WholeBuilding ccupancy == HighRiseResidential Or HotelMotel  by default result = false and unchecked;  If user checks it, the result = true, 
If result == true Section H is triggered 
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="B01c_NotApplicableMessage">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If A04_WholeBuilding ccupancy != HighRiseResidential Or HotelMotel result = BlankGrayedOut stored in NotApplicableMessage</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="B01d_IsProjectSmartTstatPlusEE_Measure" type="comp:IsProjectSmartTstatPlusEE_Measure">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Exception to Solar Ready Area: Smart Thermostat and Alternative Energy Efficiency Measure</xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">The project is a multifamily occupancy where all thermostats in each dwelling unit comply with 
                                       <d:t24ref h="110.12(a)"/>  AND at least one additional measure listed in Exception 4 to 
                                       <d:t24ref h="110.10(b)1B"/> is installed, as documented in Table I.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">User input
By default result = false and unchecked.  If user checks it, the result = true, 
 
If result == true Section I is triggered 
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="B01d_NotApplicableMessage">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If A04_WholeBuildingOccupancy != HighRiseResidential  result = BlankGrayedOut stored in NotApplicableMessage</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:element name="B01e_RoofTrafficSRA" type="comp:RoofTrafficSRA">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to Solar Ready Area: Roof is designed for vehicular traffic, parking or for heliport</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Plan sheet showing roof designed for vehicular traffic, parking or heliport</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">By default result = false and unchecked.  If user checks it, the result = true
 
If result == true 
C09_DoesProjectComplySRA == ExceptionalConditionsSRA
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="B01e_BuildingPlanName" type="comp:BuildingPlanName" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">If B01e_RoofTrafficSRA == true this field is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="B01f_RoofTooSmallSRA" type="comp:RoofTooSmallSRA">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to Solar Ready Area: Roof too small</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project is new construction and has a total roof area 
                                    <d:lte/> 533 square feet<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
If A05_RoofConstRangeSolar == NewRoofLTE533ft2
result = true 
Else 
result = false
End If
 
If result == true 
C09_DoesProjectComplySRA == ExceptionalConditionsSRA
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="B01g_StoriesTooManySRA" type="comp:StoriesTooManySRA">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to Solar Ready Area: Number of building stories</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project is nonresidential 
                                    <d:gt/> 3 stories or multifamily/ hotel/motel 
                                    <d:gt/> 10 stories.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
If  A06_BuildingStoryRange == StoriesGT10
If A04_WholeBuildingOccupancy == HighRiseResidential Or HotelMotel  
result = true
Else
result = false
End If
Else If A06_BuildingStoryRange == Stories4to10 Or StoriesGT10
result = true
Else 
result = false
End If
 
If result == true 
C09_DoesProjectComplySRA == ExceptionalConditionsSRA
Else If result == false 
field NOT REPORTED IN PDF
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="BEndNote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>1</d:sup>FOOTNOTE: Buildings with roof area 
                                       <d:lte/>533 ft<d:sup>2</d:sup> would have a required solar zone 
                                       <d:lt/> 80 ft<d:sup>2</d:sup> and are therefore exempt per 
                                       <d:t24refnolink/>110.10(b)1. </d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Bpv" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance with Solar Photovoltaic (PV) and Battery Requirements in 
                           <d:t24refnolink/>140.10/ 
                           <d:t24refnolink/>170.2(g and h)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If A04_WholeBuildngOccupancy == HotelMotel, HighRiseResidential, GroceryStoreBuilding, OfficeBuilding, FinancialInstitutionBuilding, UnleasedTenantSpace, RetailBuilding, SchoolBuilding, Warehouse, AuditoriumBuilding, ConventionCenterBuilding, LibraryBuilding, MedicalClinicBuilding, RestaurantBuilding, Or TheaterBuilding
And A05_RoofConstRangeSolar == NewRoofLTE533ft2 Or NewRoofGT533ft2
 
If this subsection is required,  one and only one field can be checked with a result == true and it is the only field that is displayed in the final report.    Fields are evaluated in the order they occur in the schema and once a field is evaluated as true and displayed as checked, remaining fields are set to unchecked, result == false and not displayed.</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Bpv01a_SystemIncludesPV" type="comp:SystemIncludesPV">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Provided PV system and battery storage sized per 
                                    <d:t24refnolink/>140.10/ 
                                    <d:t24refnolink/>170.2 (g and h)</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project has included an installed PV system and battery storage system per requirements in 
                                    <d:t24refnolink/>140.10/ 
                                    <d:t24refnolink/>170.2(g and h) as documented in Table J.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: In Section_J 
If Pv10_PV_DesignSize GTE Pv09_PV_MinimumSize And
Bat07a_BatteryCapacity GTE Bat06a_BatteryCapacityMinTotal And
Bat07b_BatteryPower GTE Bat06a_BatteryPowerMinTotal 
result = true
Else result = false
End If
 
If result == false   
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv01b_AccessAreaTooSmall" type="comp:AccessAreaTooSmall">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: Not enough Solar Access Roof Area</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The total of all available Solar Access Roof Area(s) of the project site is less than three percent of the conditioned floor area  as documented in Table J.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field:
If Bpv01a_SystemIncludesPV == true
result = false
Else (evaluating values from J)
If SUM ALL (Pv07_SolarAccessRoofArea)  LT (0.03 x SUM ALL (Pv02_ConditionedFloorArea)) And therefore Pv09_PV_MinimumSize  == 0   
result = true
Else result = false
End If
If result == true 
BSolarReady subsection is triggered
Else If result == false 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv01c_RequiredLT4kW" type="comp:RequiredLT4kW">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: Required PV 
                                    <d:lt/> 4kW</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The required PV system size is less than 4 kW dc as documented in Table J..</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: In Section_J
If Bpv01a_SystemIncludesPV == true Or Bpv01b_AccessAreaTooSmall == true
result = false
Else (evaluating values from J) 
If (Sum All (Pv08_PV_MinimumSizeSARA)) LT 4
And therefore Pv09_PV_MinimumSize  == 0 
result = true
Else result = false
End If
If result == true 
BSolarReady subsection is triggered
Else If result == false 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv01d_NoContiguousAccess" type="comp:NoContiguousAccess">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: No contiguous Solar Access Roof Area</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The Solar Access Roof Area(s) of the project site contains less than 80 contiguous square feet as documented in Table J.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: In Section_J
If Bpv01a_SystemIncludesPV == true Or Bpv01b_AccessAreaTooSmall == true Or Bpv01c_RequiredLT4kW == true
result = false
Else (evaluating values from J)
If SUM ALL (Pv07_SolarAccessRoofArea)  LT 80
And therefore Pv09_PV_MinimumSize  == 0 
result = true
Else result = false
End If
If result == true 
BSolarReady subsection is triggered
Else If result == false 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv01e_SnowloadException" type="comp:SnowloadException">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: Can't meet snow load</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project has a roof design where the enforcement authority has verified it is not possible for the PV system, including panels, modules, components, supports, and attachments to the roof structure, to meet ASCE 7-16 Chapter 7, Snow Loads.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input
If result == true 
BSolarReady subsection is triggered
Else If result == false 
Section_J is triggered And 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv0f_MultiTenantException" type="comp:MultiTenantException">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Exception to PV and Battery: Multi-tenant without VNEM or Community Solar</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project is a multi-tenant building in an area where a load serving entity does not provide either a Virtual Net Metering (VNEM) or community solar program.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input
If result == true 
BSolarReady subsection is triggered
Else If result == false 
Section_J is triggered And 
field NOT REPORTED IN PDF 
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpv0g_PerfTradeOffException" type="comp:PerfTradeOffException">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">The prescriptive PV/battery requirement has been traded off using the performance compliance approach as documented on the PRF Certificate of Compliance form.</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The project is a multi-tenant building in an area where a load serving entity does not provide either a Virtual Net Metering (VNEM) or community solar program.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input
If result == true 
BSolarReady subsection is triggered
Else If result == false And Bpv0e_SnowLoadException And Bpv0f_MultiTenantException == false 
Section_J is triggered  
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
