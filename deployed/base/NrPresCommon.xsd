         <xsd:enumeration value="ResidentialNR"/>
         <xsd:enumeration value="StateBuilding"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="TotalConditionedFloorArea">
      <xsd:annotation>
         <xsd:documentation>All conditioned floor area in the Building project.  It can be derived from all conditioned area in the Zones  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Total Conditioned Floor Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalGrossFloorArea">
      <xsd:annotation>
         <xsd:documentation>BuildlingGrossTotalFloorArea is the gross floor area of the building measured from exterior surface to exterior surface and including all reas inside the building envelope.  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Building Total Floor Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalStoryCount">
      <xsd:annotation>
         <xsd:documentation>The total number of stories in a building and includes both above grade stories and any below grade stories.  Units: none</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Total Story Count</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TotalUnconditionedFloorArea">
      <xsd:annotation>
         <xsd:documentation>All unconditioned floor area in the Building project.  It can be derived by summing Zone Floor Area of all  Zones with Space Conditioning Type set to Unconditioned  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Total Unconditioned Floor Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="TypeOtherDescription">
      <xsd:annotation>
         <xsd:documentation>Text describing type not in list</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token"/>
   </xsd:simpleType>
   <xsd:simpleType name="UFactor">
      <xsd:annotation>
         <xsd:documentation>The overall coefficient of thermal transmittance through a layer or construction assembly. It includes air film resistance at both surfaces.  Units: Btu per h ft2 F</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>UFactor</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal4PlacesNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="UFactorCenterOfGlass">
      <xsd:annotation>
         <xsd:documentation>The overall coefficient of thermal transmittance through glass of a fenestration product.  Units: Btu per h ft2 F</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal4PlacesNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="UFactorLimit">
      <xsd:annotation>
         <xsd:documentation>This is the regulatory limit for the UFactor of a building element  Units: Btu per h ft2 F</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal4PlacesNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="UFactorLimitSkylight">
      <xsd:annotation>
         <xsd:documentation>This is the regulatory limit for the UFactor of a Skylight fenestration  Units: Btu per h ft2 F</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal3PlacesNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="VT">
      <xsd:annotation>
         <xsd:documentation>Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal4PlacesNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="VT_CenterOfGlass">
      <xsd:annotation>
         <xsd:documentation>The fraction of incident visible radiation that is transmitted by the material. Visible radiation includes only a sub-range of solar radiation and excludes infrared and ultraviolet wavelengths.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal4PlacesNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="VT_Limit">
      <xsd:annotation>
         <xsd:documentation>Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal4PlacesNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="VentilationPerSpace">
      <xsd:annotation>
         <xsd:documentation>Units: ft3 per min</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative"/>
   </xsd:simpleType>
   <xsd:simpleType name="VentilationRate">
      <xsd:annotation>
         <xsd:documentation>The amount of outside air provided to a space during occupied hours divided by the floor area of that space.  Units: ft3 per min ft2</xsd:documentation>
