                                                      <xsd:element name="ParkInScope" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Park01_InstalledSameAsCC" type="comp:InstalledSameAsCC"  minOccurs="0">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                                     <xsd:documentation source="AdditionalRequirements">[d:i]This table includes all newly installed mechanical ventilation systems which serve parking garages and have a design exhaust rate greater than or equal to 10,000 cfm.  The inputs of Table H are used to demonstrate compliance with the requirements of [d:t24refnolink/]120.6(c) [/d:i]</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as NRCC value  Or false if installed is different than NRCC value</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Park02cc_ParkingGarageArea" type="com:ParkingGarageArea" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Parking Garage Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If NRCC-PRC parent
If H11_ParkingGarageArea IsPresent
result = H11_ParkingGarageArea
End If
End If</xsd:documentation>
                                                                        <xsd:appinfo source="CIsrc">
                                                                           <dtyp:srcSchema form="NRCCPRC01E"  usage="copy">H11_ParkingGarageArea</dtyp:srcSchema>
                                                                        </xsd:appinfo>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Park02cc_NotApplicableMessage" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Parking Garage Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If ParkCtrl01cc_VehiclesGT20PctNonGas == true  Or ParkCtrl02cc_NewExhaustLT10000cfm == true 
result = BlankGrayedOut</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
