                                    <xsd:element name="Rcon08_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Condenser Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete  
 Else If Rcon01_InMyScope == false 
result = NotInScope
 
Else If 
Rcon04_CondenserTempControls ==  Rcon04cc_CondenserTempControls 
And
Rcon05_VariableSpeedControl == Rcon05cc_VariableSpeedControl
 
Then If
Rcon03cc_NotApplicableMessage IsPresent Or  Rcon03_CondenserLocation ==  Rcon03cc_CondenserLocation
And
Rcon06cc_NotApplicableMessage isPresent Or Rcon06_RefrigerantType == Rcon06cc_RefrigerantType
And
Rcon07cc_NotApplicableMessage IsPresent Or Rcon07_CondenserEfficiency == Rcon07cc_CondenserEfficiency 
result = ExactMatch
Else If
Rcon03_CondenserLocation IsPresent And Rcon03_CondenserLocation !=  Rcon03cc_CondenserLocation
Or
Rcon06_RefrigerantType IsPresent And Rcon06_RefrigerantType != Rcon06cc_RefrigerantType
Or
Rcon07_CondenserEfficiency  IsPresent And Rcon07_CondenserEfficiency LT Rcon07cc_CondenserEfficiency 
result = MayNotComply
Else
result = EqallBetter
End If
End If</xsd:documentation>
