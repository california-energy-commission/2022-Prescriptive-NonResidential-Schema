                                                               <xsd:documentation source="CalculationsAndRules">If NRCC-PRC parent
      And Ups06_NotApplicableMessage isPresent
      result = BlankGrayedOut
      End If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="NRCCPRC01E"  usage="copy">Ups06_NotApplicableMessage</dtyp:srcSchema>
