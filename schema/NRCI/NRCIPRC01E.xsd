                                    <xsd:element name="Hoods07_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Kitchen Exhaust Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any fields Hoods02-Hoods06 are not ompleted
result = TableIncomplete
 Else If Hoods01_InMyScope == false 
result = NotInScope
 
Else If
Hoods02_KitchenHoodType = Hoods02cc_KitchenHoodType
And
Hoods03_NotApplicableMessage  IsPresent Or Hoods03_KitchenExhaustHoodStyle == Hoods03cc_KitchenExhaustHoodStyle
And
Hoods04_NotApplicableMessage IsPresent Or Hoods04_KitchenExhaustHoodDuty == Hoods04cc_KitchenExhaustHoodDuty
Then If 
 Hoods05_NotApplicableMessage IsPresent Or Hoods05_KitchenExhaustHoodLength == Hoods05cc_KitchenExhaustHoodLength
And 
Hoods06_KitchenExhaustHoodFlow == Hoods06cc_KitchenExhaustHoodFlow
result = ExactMatch
Else If
Hoods05_KitchenExhaustHoodLength IsPresent  Hoods05_KitchenExhaustHoodLength LTE Hoods05cc_KitchenExhaustHoodLength
And 
Hoods06_KitchenExhaustHoodFlow LTE Hoods06cc_KitchenExhaustHoodFlow
result = EqualBetter
Else If
Hoods03_KitchenExhaustHoodStyle IsPfesent And Hoods03_KitchenExhaustHoodStyle != Hoods03cc_KitchenExhaustHoodStyle
Or 
Hoods04_KitchenExhaustHoodDuty IsPresent And Hoods04_KitchenExhaustHoodDuty != Hoods04cc_KitchenExhaustHoodDuty
result = MayNotComply
End If
End If</xsd:documentation>
