                                                               <xsd:documentation source="CalculationsAndRules">If NRCC-PRC parent
      If Ups06_UpsEfficiency isPresent
      result = Ups06_UpsEfficiency
      End If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="NRCCPRC01E"  usage="copy">Ups06_UpsEfficiency</dtyp:srcSchema>
